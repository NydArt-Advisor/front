{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:simple": "node scripts/simple-test.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "docker:build": "docker build -t nydart-advisor/frontend .",
    "docker:run": "docker run -p 3000:3000 nydart-advisor/frontend",
    "docker:push": "docker push nydart-advisor/frontend",
    "ci:test": "npm run lint && npm run type-check && npm run security:audit && npm run test:ci",
    "ci:build": "npm run ci:test && npm run build && npm run docker:build",
    "ci:deploy": "npm run ci:build && npm run docker:push"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "dotenv": "^17.0.1",
    "framer-motion": "^11.0.3",
    "next": "14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-icons": "^5.5.0",
    "react-phone-input-2": "^2.15.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.0"
  }
}